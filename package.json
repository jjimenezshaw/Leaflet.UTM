{
  "name": "leaflet.utm",
  "version": "1.0.0",
  "description": "Converts LatLng to UTM WGS84 in Leaflet",
  "main": "L.LatLng.UTM.js",
  "types": "L.LatLng.UTM.d.ts",
  "directories": {
    "test": "test"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "debug": "http-server -c-1",
    "lint:js": "eslint .",
    "lint:ts": "eslint --config eslint.config-ts.mjs *.ts",
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:fix": "npm run lint:js -- --fix",
    "spec:js:v1": "mocha -r jsdom-global/register test/v1/spec.js",
    "spec:js": "mocha -r jsdom-global/register test/spec.js",
    "spec:ts": "tsc",
    "spec:v1": "npm run spec:js:v1 && npm run spec:ts",
    "spec": "npm run spec:js && npm run spec:ts",
    "test:v1": "npm run lint && npm run spec:v1",
    "test": "npm run lint && npm run spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjimenezshaw/Leaflet.UTM.git"
  },
  "keywords": [
    "Leaflet",
    "map",
    "geo",
    "UTM"
  ],
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "chai": "^4.5.0",
    "eslint": "^9.37.0",
    "eslint-plugin-html": "^8.1.3",
    "http-server": "^14.1.1",
    "jsdom": "27.0.0",
    "jsdom-global": "3.0.2",
    "leaflet": "^2.0.0-alpha.1",
    "mocha": "^11.7.4",
    "proj4": "^2.19.10",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-git-version": "^0.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "peerDependencies": {
    "leaflet": ">=0.7.0"
  },
  "author": "Javier Jimenez Shaw",
  "readmeFilename": "README.md"
}
