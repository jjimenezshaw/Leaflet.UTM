{
  "name": "leaflet.utm",
  "version": "1.0.0",
  "description": "Converts LatLng to UTM WGS84 in Leaflet",
  "main": "L.LatLng.UTM.js",
  "directories": {
    "test": "test"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "lint": "eslint L.LatLng.UTM.js examples/* test/*",
    "lintfix": "eslint L.*.js examples/* test/* --fix",
