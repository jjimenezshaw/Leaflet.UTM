    "chai-leaflet": "^0.0.13",
    "eslint": "^4.16.0",
    "eslint-plugin-html": "^4.0.1",
    "leaflet": "^1.3.1",
    "mocha": "^5.0.0",
    "mocha-phantomjs-core": "^2.1.2",
    "mversion": "^1.10.1",
    "phantomjs-prebuilt": "^2.1.16",
    "proj4": "2.4.4"
  },
  "peerDependencies": {
    "leaflet": "^1.0.3"
  },
  "author": "Javier Jimenez Shaw",
  "contributors": [
    {}
